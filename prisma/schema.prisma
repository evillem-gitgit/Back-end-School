generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Student {
  id    Int    @id @default(autoincrement())
  nome  String
  email String @unique
  idade Int?
}

model Teacher {
  id    Int    @id @default(autoincrement())
  nome  String
  email String @unique
  idade Int?
}
